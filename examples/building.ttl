###################### OS Data Extension ########################
building:AccessPoint a rdfs:Class ;
    rdfs:label "Access Point" ;
    ies:powertype building:ClassOfAccessPoint ;
    rdfs:comment "A `building:BuiltEntity` that allows pedestrian or vehicular access to or from a site, consistent with Ordnance Survey's 'Site Access Location' concept." ;
    rdfs:subClassOf building:AccessPointState,
        building:BuiltEntity .

building:AccessPointState a rdfs:Class ;
    rdfs:label "Access Point State" ;
    ies:powertype building:ClassOfAccessPointState ;
    rdfs:comment "A temporal state of a `building:AccessPoint`." ;
    rdfs:subClassOf building:BuiltEntityState .

building:ClassOfAccessPoint a rdfs:Class ;
    rdfs:label "Class of Access Point Location" ;
    rdfs:comment "An `building:ClassOfLocation` that acts as a classifier for types of AccessPoint, e.g., pedestrian-only access, vehicular access, or mixed-use access points." ;
    rdfs:subClassOf building:ClassOfBuiltEntity .

building:ClassOfAccessPointState a rdfs:Class ;
    rdfs:label "Class of Access Point State" ;
    rdfs:comment "An `building:ClassOfLocationState` that acts as a classifier for types of AccessPointState, e.g., open, closed, restricted, or temporarily diverted access points." ;
    rdfs:subClassOf building:ClassOfBuiltEntityState .

# Classifier for access mode (parallel to AccessPointByLevel)
building:AccessPointByAccessMode a rdfs:Class ;
    rdfs:label "Access Point by Access Mode" ;
    rdfs:comment "A `building:ClassOfAccessPoint` that classifies `building:AccessPoint` by the mode(s) permitted at the access point (pedestrian, vehicular, or both), aligned to OS NGD accessmodevalue." ;
    rdfs:subClassOf building:ClassOfAccessPoint .

# Instances of the classifier (and subclasses of AccessPoint) per OS code list

building:AccessPedestrianOnly a building:AccessPointByAccessMode ;
    rdfs:label "Access Pedestrian Only" ;
    rdfs:comment "An instance of `building:AccessPointByAccessMode` corresponding to OS NGD `accessmodevalue` 'Pedestrian' — an entrance/exit location for pedestrians only." ;
    rdfs:subClassOf building:AccessPoint .

building:AccessVehicularOnly a building:AccessPointByAccessMode ;
    rdfs:label "Access Vehicular Only" ;
    rdfs:comment "An instance of `building:AccessPointByAccessMode` corresponding to OS NGD `accessmodevalue` 'Vehicular' — an entrance/exit location for vehicles only." ;
    rdfs:subClassOf building:AccessPoint .

building:AccessPedestrianAndVehicular a building:AccessPointByAccessMode ;
    rdfs:label "Access Pedestrian And Vehicular" ;
    rdfs:comment "An instance of `building:AccessPointByAccessMode` corresponding to OS NGD `accessmodevalue` 'Pedestrian And Vehicular' — an entrance/exit location for both pedestrians and vehicles." ;
    rdfs:subClassOf building:AccessPoint .

# Classifier for access purpose (parallel to AccessPointByLevel)
building:AccessPointByAccessPurpose a rdfs:Class ;
    rdfs:label "Access Point by Access Purpose" ;
    rdfs:comment "A `building:ClassOfAccessPoint` that classifies `building:AccessPoint` by the principal function of the access location (Emergency, Primary Public, Private, or Public), aligned to OS NGD `accesspurposevalue`." ;
    rdfs:subClassOf building:ClassOfAccessPoint .

# Instances of the classifier (and subclasses of AccessPoint) per OS code list

building:EmergencyAccess a building:AccessPointByAccessPurpose ;
    rdfs:label "Emergency Access" ;
    rdfs:comment "An instance of `building:AccessPointByAccessPurpose` corresponding to OS NGD `accesspurposevalue` 'Emergency' — an entrance/exit location that is solely intended for use in emergencies." ;
    rdfs:subClassOf building:AccessPoint .

building:PrimaryPublicAccess a building:AccessPointByAccessPurpose ;
    rdfs:label "Primary Public Access" ;
    rdfs:comment "An instance of `building:AccessPointByAccessPurpose` corresponding to OS NGD `accesspurposevalue` 'Primary Public' — a principal entrance/exit location that is intended for public use, usually referred to as the ‘Main Entrance’. A given Site/Building may have more than one primary public access location, especially for larger features." ;
    rdfs:subClassOf building:AccessPoint .

building:PrivateAccess a building:AccessPointByAccessPurpose ;
    rdfs:label "Private Access" ;
    rdfs:comment "An instance of `building:AccessPointByAccessPurpose` corresponding to OS NGD `accesspurposevalue` 'Private' — an entrance/exit location that is not intended for public use." ;
    rdfs:subClassOf building:AccessPoint .

building:PublicAccess a building:AccessPointByAccessPurpose ;
    rdfs:label "Public Access" ;
    rdfs:comment "An instance of `building:AccessPointByAccessPurpose` corresponding to OS NGD `accesspurposevalue` 'Public' — an entrance/exit location that is intended for public use." ;
    rdfs:subClassOf building:AccessPoint .

# Classifier for accessibility (parallel to AccessPointByAccessPurpose)
building:AccessPointByAccessibility a rdfs:Class ;
    rdfs:label "Access Point by Accessibility" ;
    rdfs:comment "A `building:ClassOfAccessPoint` that classifies `building:AccessPoint` by its accessibility state or impact on mobility (Facilitated, Unconstrained, or Constrained)." ;
    rdfs:subClassOf building:ClassOfAccessPoint .

# Instances of the classifier (and subclasses of AccessPoint)

building:FacilitatedAccessPoint a building:AccessPointByAccessibility ;
    rdfs:label "Facilitated Access" ;
    rdfs:comment "An instance of `building:AccessPointByAccessibility` — an access point whose features facilitate mobility for many users (e.g., ramp with appropriate slope, automatic door)." ;
    rdfs:subClassOf building:AccessPoint .

building:UnconstrainedAccessPoint a building:AccessPointByAccessibility ;
    rdfs:label "Unconstrained Access" ;
    rdfs:comment "An instance of `building:AccessPointByAccessibility` — an access point with no notable features expected to significantly constrain or facilitate mobility for most users." ;
    rdfs:subClassOf building:AccessPoint .

building:ConstrainedAccessPoint a building:AccessPointByAccessibility ;
    rdfs:label "Constrained Access" ;
    rdfs:comment "An instance of `building:AccessPointByAccessibility` — an access point whose features are likely to constrain mobility for some users (e.g., step, narrow doorway, steep gradient)." ;
    rdfs:subClassOf building:AccessPoint .

building:accessibilityImpactedBy a owl:ObjectProperty;
    rdfs:label "accessibility impacted by" ;
    rdfs:comment "A relationship that obtains between an instance of `building:AccessPoint` and `building:BuiltEntity` whose existence influences the accessibility of that `building:AccessPoint` (positively or negatively)." ;
    rdfs:domain building:AccessPointState ;
    rdfs:range building:BuiltEntityState .

