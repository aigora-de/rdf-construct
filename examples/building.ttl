building:LychGate a rdfs:Class ;
  rdfs:label "Lych Gate" ;
  rdfs:comment "A Structure that is a roofed gateway typically at the entrance to a churchyard." ;
  rdfs:subClassOf building:Structure .

building:MotorVehicleWeighbridge a rdfs:Class ;
  rdfs:label "Motor Vehicle Weighbridge" ;
  rdfs:comment "A Structure that houses apparatus for weighing road vehicles and their loads." ;
  rdfs:subClassOf building:Structure .

building:RailVehicleWeighbridge a rdfs:Class ;
  rdfs:label "Rail Vehicle Weighbridge" ;
  rdfs:comment "A Structure that houses apparatus for weighing rail vehicles and their loads." ;
  rdfs:subClassOf building:Structure .

building:RoofedClockTower a rdfs:Class ;
  rdfs:label "Roofed Clock Tower" ;
  rdfs:comment "A tower incorporating a clock and covered by a roofed superstructure." ;
  rdfs:subClassOf building:Tower .

building:RoofedShaft a rdfs:Class ;
  rdfs:label "Roofed Shaft" ;
  rdfs:comment "A Structure that encloses the vertical or near‑vertical entrance to a mine." ;
  rdfs:subClassOf building:Structure .

building:RoofedTower a rdfs:Class ;
  rdfs:label "Roofed Tower" ;
  rdfs:comment "A tall, isolated tower covered by a roof and lacking an explicit functional specification." ;
  rdfs:subClassOf building:Tower .

building:RoofedWell a rdfs:Class ;
  rdfs:label "Roofed Well" ;
  rdfs:comment "A Structure that is a water‑extraction well provided with a protective roof." ;
  rdfs:subClassOf building:Structure .

building:Stand a rdfs:Class ;
  rdfs:label "Stand" ;
  rdfs:comment "A Structure that provides tiered seating and/or standing accommodation for spectators, with at least one open side." ;
  rdfs:subClassOf building:Structure .

building:TelecommunicationsMast a rdfs:Class ;
  rdfs:label "Telecommunications Mast" ;
  rdfs:comment "A Structure that is a tall mast supporting equipment for transmitting or receiving electronic signals." ;
  rdfs:subClassOf building:Structure .

building:Terraces a rdfs:Class ;
  rdfs:label "Terraces" ;
  rdfs:comment "A Structure that comprises banked, wide steps forming spectator accommodation with at least one open side." ;
  rdfs:subClassOf building:Structure .

building:TravellingCrane a rdfs:Class ;
  rdfs:label "Travelling Crane" ;
  rdfs:comment "A Structure that houses rail‑mounted lifting machinery capable of moving loads along a track." ;
  rdfs:subClassOf building:Structure .

building:VehicleDip a rdfs:Class ;
  rdfs:label "Vehicle Dip" ;
  rdfs:comment "A Structure that contains a sunken basin used for immersing vehicles in disinfectant solution." ;
  rdfs:subClassOf building:Structure .

building:WaterPumpingChamber a rdfs:Class ;
  rdfs:label "Water Pumping Chamber" ;
  rdfs:comment "A Structure that contains a void or chamber from which fresh water is pumped to regulate supply." ;
  rdfs:subClassOf building:Structure .

building:WaterTower a rdfs:Class ;
  rdfs:label "Water Tower" ;
  rdfs:comment "A tall tower supporting a water tank to provide pressure and regulate supply." ;
  rdfs:subClassOf building:Tower .

building:Weir a rdfs:Class ;
  rdfs:label "Weir" ;
  rdfs:comment "A Structure that is a barrier built across a watercourse to control or divert flow." ;
  rdfs:subClassOf building:Structure .

building:Well a rdfs:Class ;
  rdfs:label "Well" ;
  rdfs:comment "A Structure that is a constructed shaft or hole for extracting water from the ground." ;
  rdfs:subClassOf building:Structure .

building:WindPump a rdfs:Class ;
  rdfs:label "Wind Pump" ;
  rdfs:comment "A Structure that is a tower or mast supporting wind‑driven blades used to pump water." ;
  rdfs:subClassOf building:Structure .

building:Arch a rdfs:Class ;
  rdfs:label "Arch" ;
  rdfs:comment "A BuiltEntity that is a curved structural element spanning an opening and bearing load from above." ;
  rdfs:subClassOf building:BuiltEntity .

building:Archway a rdfs:Class ;
  rdfs:label "Archway" ;
  rdfs:comment "A BuiltEntity that is a roofed passage opening formed within a Structure allowing vehicular passage." ;
  rdfs:subClassOf building:BuiltEntity .

building:Pergola a rdfs:Class ;
  rdfs:label "Pergola" ;
  rdfs:comment "A BuiltEntity that is a colonnaded passage supporting a trellised roof intended for planting." ;
  rdfs:subClassOf building:BuiltEntity .

building:RoofedChimney a rdfs:Class ;
  rdfs:label "Roofed Chimney" ;
  rdfs:comment "A BuiltEntity that is a flue or chimney provided with a protective roof or capping." ;
  rdfs:subClassOf building:BuiltEntity .

building:RoofedLeat a rdfs:Class ;
  rdfs:label "Roofed Leat" ;
  rdfs:comment "A BuiltEntity that is a watercourse enclosed by a conventional roof, conveying water for processes." ;
  rdfs:subClassOf building:BuiltEntity .

building:RoofedOutfall a rdfs:Class ;
  rdfs:label "Roofed Outfall" ;
  rdfs:comment "A BuiltEntity that is a roofed channel or structure used to discharge liquids to a receiving water body." ;
  rdfs:subClassOf building:BuiltEntity .

building:SupportedRoofedConveyor a rdfs:Class ;
  rdfs:label "Supported Roofed Conveyor" ;
  rdfs:comment "A BuiltEntity that is an elevated, roofed conveyor installation for transporting materials." ;
  rdfs:subClassOf building:BuiltEntity .

building:VentilationShaft a rdfs:Class ;
  rdfs:label "Ventilation Shaft" ;
  rdfs:comment "A BuiltEntity that is a vertical passage constructed to allow air flow to or from tunnels or underground spaces." ;
  rdfs:subClassOf building:BuiltEntity .

building:Waterwheel a rdfs:Class ;
  rdfs:label "Waterwheel" ;
  rdfs:comment "A BuiltEntity that is a large wheel driven by flowing water to transmit mechanical power." ;
  rdfs:subClassOf building:BuiltEntity .

building:BuildingWithMadeSurface a rdfs:Class ;
  rdfs:label "Building And Made Surface" ;
  rdfs:comment "A BuiltEntity that is a man‑made surface which simultaneously serves as the roof to a Building below." ;
  rdfs:subClassOf building:BuiltEntity .

building:BuildingUnderConstruction a rdfs:Class ;
  rdfs:label "Building Under Construction" ;
  rdfs:comment "A StructureState that is the temporal condition of a Building while construction is in progress and completion has not yet occurred." ;
  rdfs:subClassOf building:StructureState .

building:RuinedBuilding a rdfs:Class ;
  rdfs:label "Ruined Building" ;
  rdfs:comment "A StructureState that is the temporal condition of a Building when much of its fabric has decayed or collapsed so it is no longer fully functional." ;
  rdfs:subClassOf building:StructureState .

building:BuildingWithCarParkOnRoof a rdfs:Class ;
  rdfs:label "Building With Car Park On Roof" ;
  rdfs:comment "A StructureState that is the temporal condition of a Building whose roof is used as a car‑parking surface." ;
  rdfs:subClassOf building:StructureState .

building:BuildingWithPathOnRoof a rdfs:Class ;
  rdfs:label "Building With Path On Roof" ;
  rdfs:comment "A StructureState that is the temporal condition of a Building whose roof provides a pedestrian pathway." ;
  rdfs:subClassOf building:StructureState .

building:BuildingWithDovecot a rdfs:Class ;
  rdfs:label "Building And Dovecot" ;
  rdfs:comment "A StructureState that is the temporal condition of a Building equipped with internal provision for dovecote roosting." ;
  rdfs:subClassOf building:StructureState .

building:BuildingWithLight a rdfs:Class ;
  rdfs:label "Building And Light" ;
  rdfs:comment "A StructureState that is the temporal condition of a Building equipped with a light for illuminating surrounding areas." ;
  rdfs:subClassOf building:StructureState .

building:FloatingStructure a rdfs:Class ;
  rdfs:label "Floating Structure" ;
  rdfs:comment "A StructureState that is the temporal condition of a Structure situated afloat upon the surface of water." ;
  rdfs:subClassOf building:StructureState .

###################### Named Location Classes #######################

building:LocationAsBuildingLine a rdfs:Class ;
    rdfs:label "Location As Building line" ;
    rdfs:comment "An `building:ClassOfLocation`" ;
    rdfs:subClassOf building:ClassOfLocationState .  # class of state


### Building Internal Division
building:BuildingInternalDivision
  a rdfs:Class, building:LocationAsBuildingLine ;
  rdfs:subClassOf ies:Location ; # State ;
  rdfs:label "Building Internal Division"@en ;
  rdfs:comment "An ies:Location that denotes the alignment of a division internal to a building complex—either between two buildings with the same occupier, between individual garages within a block, or between buildings that have an overhanging outline or a shared occupation."@en ;
  dct:source <https://docs.os.uk/osngd/code-lists/code-lists-overview/buildinglinedescriptionvalue> .

### Building Occupier Division
building:BuildingOccupierDivision
  a rdfs:Class, building:LocationAsBuildingLine ;
  rdfs:subClassOf ies:Location ;
  rdfs:label "Building Occupier Division"@en ;
  rdfs:comment "An ies:Location that denotes the alignment of the division between two buildings that have different occupiers."@en ;
  dct:source <https://docs.os.uk/osngd/code-lists/code-lists-overview/buildinglinedescriptionvalue> .

### Overhanging Building Edge
building:OverhangingBuildingEdge
  a rdfs:Class, building:LocationAsBuildingLine ;
  rdfs:subClassOf ies:Location ;
  rdfs:label "Overhanging Building Edge"@en ;
  rdfs:comment "An ies:Location that denotes the edge delimiting the limit of a building element that overhangs and allows passage underneath."@en ;
  dct:source <https://docs.os.uk/osngd/code-lists/code-lists-overview/buildinglinedescriptionvalue> .


##############################
# OS Geographic concepts     #
##############################

building:SovereignCountry a rdfs:Class ;
    rdfs:label "Sovereign Country" ;
    rdfs:comment "A country that is the whole region ruled by a sovereign state, and is not a region of another country; a few examples include the United Kingdom, Italy, Japan, Kenya, and the USA; England, Scotland, Wales, and Northern Ireland are not sovereign countries, but rather constituent countries" ;
    rdfs:subClassOf ies:Country .

building:ConstituentCountry a rdfs:Class ;
    rdfs:label "Constituent Country" ;
    rdfs:comment "A country that is a region of another country; constituent countries have typically been devolved a high degree of autonomy because of its distinctive historical and cultural identity; England, Scotland, Wales, and Northern Ireland are the four constituent countries of the United Kingdom of Great Britain and Northern Ireland; other examples of constituent countries include Denmark, Greenland, and the Faroe Islands, which are the three constituent countries of the Kingdom of Denmark" ;
    rdfs:subClassOf ies:Country, ies:RegionOfCountry .

building:UKLocalAdministrativeRegion a rdfs:Class ;
   rdfs:label "UK Local Administrative Region" ;
   rdfs:comment "A region of the UK that is an administrative division of one of its four constituent countries; local administrative regions have different names depending on the constituent countries in which they are located" ;
   rdfs:subClassOf ies:RegionOfCountry .

building:UpperTierLocalAdministrativeRegion a rdfs:Class ;
   rdfs:label "Upper-Tier Local Administrative Region" ;
   rdfs:comment "A UK local administrative region that is a direct administrative division of one of the constituent countries of the United Kingdom; the councils of upper-tier local administrative regions are responsible for strategic, high cost services in the related region; some - which are called 'unitary authorities' -  are also responsible for more localized services, hence serving all government functions in the region" ;
   rdfs:subClassOf building:UKLocalAdministrativeRegion .

building:LowerTierLocalAdministrativeRegion a rdfs:Class ;
   rdfs:label "Lower-Tier Local Administrative Region" ;
   rdfs:comment "A UK local administrative region that is not a direct administrative division of one of the constituent countries of the United Kingdom; the councils of lower-tier local administrative regions are only responsible for localized services like housing, waste collection, and leisure" ;
   rdfs:subClassOf building:UKLocalAdministrativeRegion .

building:UnitaryAuthorityRegion a rdfs:Class ;
   rdfs:label "Unitary Authority Region" ;
   rdfs:comment "An upper-tier local administrative region whose administration is responsible for all government functions and services in a given area; informally, a unitary authority can be said to combine the functions of a county council and those of a district council" ;
   rdfs:subClassOf building:UpperTierLocalAdministrativeRegion .

building:EnglishAdministrativeCounty a rdfs:Class ;
   rdfs:label "English Administrative County" ;
   rdfs:comment "An upper-tier local administrative region that (i) is a direct administrative division of England, and (ii) is further divided into districts; county councils are responsible for strategic, higher cost services like education, social services, and transport, whereas more localized services are responsibility of district councils" ;
   rdfs:subClassOf building:UpperTierLocalAdministrativeRegion .

building:MetropolitanCounty a rdfs:Class ;
   rdfs:label "Metropolitan County" ;
   rdfs:comment "An English administrative county that approximately corresponds to one large town or city" ;
   rdfs:subClassOf building:EnglishAdministrativeCounty .

building:NonMetropolitanCounty a rdfs:Class ;
   rdfs:label "Non-Metropolitan County" ;
   rdfs:comment "An English administrative county that does not correspond to one large town or city, and typically encompasses various settlements and rural areas" ;
   rdfs:subClassOf building:EnglishAdministrativeCounty .

building:ScottishCouncilArea a rdfs:Class ;
   rdfs:label "Scottish Council Area" ;
   rdfs:comment "An upper-tier local administrative region that is a direct administrative division of Scotland; all Scottish council areas are unitary authority regions" ;
   rdfs:subClassOf building:UpperTierLocalAdministrativeRegion, building:UnitaryAuthorityRegion .

building:WelshPrincipalArea a rdfs:Class ;
   rdfs:label "Welsh Principal Area" ;
   rdfs:comment "An upper-tier local administrative region that is a direct administrative division of Wales; all Welsh principal areas are unitary authority regions" ;
   rdfs:subClassOf building:UpperTierLocalAdministrativeRegion, building:UnitaryAuthorityRegion .

building:NorthernIrelandDistrict a rdfs:Class ;
   rdfs:label "Northern Ireland District" ;
   rdfs:comment "An upper-tier local administrative region that is a direct administrative division of Northern Ireland; all Northern Ireland Districts are unitary authority regions" ;
   rdfs:subClassOf building:UpperTierLocalAdministrativeRegion, building:UnitaryAuthorityRegion .

building:EnglishDistrict a rdfs:Class ;
   rdfs:label "English District" ;
   rdfs:comment "A lower-tier local administrative region that is a direct administrative division of an English administrative county" ;
   rdfs:subClassOf building:LowerTierLocalAdministrativeRegion .

building:CivilParish a rdfs:Class ;
   rdfs:label "Civil Parish" ;
   rdfs:comment "A lower-tier local administrative region that is a direct administrative division of either a unitary authority or an English district; civil parishes are the lowest-level administrative divisions of England, since they have no smaller administrative divisions; civil parishes are typically - but not exclusively - found in rural areas" ;
   rdfs:subClassOf building:LowerTierLocalAdministrativeRegion, building:CivilParishOrCommunityArea .

building:WelshCommunityArea a rdfs:Class ;
   rdfs:label "Welsh Community Area" ;
   rdfs:comment "A lower-tier local administrative region that is a direct administrative division of a Welsh principal area; community areas can be viewed as the Welsh analog of English civil parishes and have similarly localized responsibilities" ;
   rdfs:subClassOf building:LowerTierLocalAdministrativeRegion, building:CivilParishOrCommunityArea .

building:CivilParishOrCommunityArea a rdfs:Class ;
   rdfs:label "Civil Parish Or Community Area" ;
   rdfs:comment "A lower-tier local administrative region that is either an English civil parish or a Welsh community area" ;
   rdfs:subClassOf building:LowerTierLocalAdministrativeRegion .

building:ConstituencyRegion a rdfs:Class ;
   rdfs:label "Constituency Region" ;
   rdfs:comment "A region of a country that is instituted for enabling the residing voters to elect their representatives in either the central government of the country or in some local administration" ;
   rdfs:subClassOf ies:RegionOfCountry .

building:UKParliamentConstituencyRegion a rdfs:Class ;
   rdfs:label "UK Parliamentary Constituency Region" ;
   rdfs:comment "A constituency that is instituted for electing one member of the House of Commons in each UK general election" ;
   rdfs:subClassOf building:ConstituencyRegion .

building:DevolvedParliamentConstituencyRegion a rdfs:Class ;
   rdfs:label "Devolved Parliament Constituency Region" ;
   rdfs:comment "A constituency that is instituted for electing members of one of the three devolved parliaments of the United Kingdom; the devolved parliaments of the United Kingdom are the Scottish Parliament, the Welsh Parliament (aka Senedd Cymru), and the Northern Ireland Assembly" ;
   rdfs:subClassOf building:ConstituencyRegion .

building:ScottishParliamentConstituencyRegion a rdfs:Class ;
   rdfs:label "Scottish Parliament Constituency Region" ;
   rdfs:comment "A constituency that is instituting for electing one member of the Scottish Parliament in each Scottish Parliament election" ;
   rdfs:subClassOf building:DevolvedParliamentConstituencyRegion .

building:SeneddConstituencyRegion a rdfs:Class ;
   rdfs:label "Senedd Constituency Region" ;
   rdfs:comment "A constituency that is instituted for electing one member of the Welsh Parliament (aka Senedd Cymru) in each Senedd election" ;
   rdfs:subClassOf building:DevolvedParliamentConstituencyRegion .

building:AssemblyConstituencyRegion a rdfs:Class ;
   rdfs:label "Assembly Constituency Region" ;
   rdfs:comment "A constituency that is instituted for electing one member of the Northern Ireland Assembly in each Northern Ireland Assembly election" ;
   rdfs:subClassOf building:DevolvedParliamentConstituencyRegion .

building:Ward a rdfs:Class ;
   rdfs:label "Ward" ;
   rdfs:comment "A constituency that is instituted for electing one or more representatives in local elections" ;
   rdfs:subClassOf building:ConstituencyRegion .

building:CeremonialCounty a rdfs:Class ;
   rdfs:label "Ceremonial County" ;
   rdfs:comment "A region of a the UK that is instituted for appointing a Lord Lieutenant and possibly one or two High Sheriffs, who serve respectively as personal and judicial representative of the regnant king or queen; many ceremonial counties approximately correspond or are even identical to administrative regions of the UK" ;
   rdfs:subClassOf ies:RegionOfCountry .

building:EnglishCeremonialCounty a rdfs:Class ;
   rdfs:label "English Ceremonial County" ;
   rdfs:comment "A ceremonial county that is part of England; each English ceremonial county has one Lord Lieutenant and one or two High Sheriffs" ;
   rdfs:subClassOf building:CeremonialCounty .

building:ScottishLieutenancyArea a rdfs:Class ;
   rdfs:label "Scottish Lieutenancy Area" ;
   rdfs:comment "A ceremonial county that is part of Scotland; each Scottish Lieutenancy Area has one Lord Lieutenant, but no High Sheriff" ;
   rdfs:subClassOf building:CeremonialCounty .

building:WelshPreservedCounty a rdfs:Class ;
   rdfs:label "Welsh Preserved County" ;
   rdfs:comment "A ceremonial county that is part of Wales; each Welsh Preserved County has one Lord Lieutenant and one High Sheriff" ;
   rdfs:subClassOf building:CeremonialCounty .

building:NorthernIrelandLieutenancyArea a rdfs:Class ;
   rdfs:label "Northern Ireland Lieutenancy Area" ;
   rdfs:comment "A ceremonial county that is part of Northern Ireland; each Northern Ireland Lieutenancy Area has one Lord Lieutenant, but no High Sheriff" ;
   rdfs:subClassOf building:CeremonialCounty .

building:HistoricCounty a rdfs:Class ;
   rdfs:label "Historic County" ;
   rdfs:comment "A region of the UK that was initially instituted as a feudal or administrative division in a time span that goes from the Early Middle Ages to the Tudor period; historic counties are linked to cultural and local identities, and are still referred to for certain purposes, like sport initiatives or even judicial purposes" ;
   rdfs:subClassOf ies:RegionOfCountry .

building:GeopoliticalDependency a rdfs:Class ;
   rdfs:label "Geopolitical Dependency" ;
   rdfs:comment "A location that (i) is not part of a sovereign country, and (ii) is in some respects under the jurisdiction of the institutions of a sovereign country; examples include Puerto Rico (which is a dependency of the USA), the Cook Islands (which are a dependency of New Zealand), and the British Overseas Territories - and the Falklands - which are dependencies of the United Kingdom" ;
   rdfs:subClassOf ies:Location .

building:isGeopoliticalDependencyOf a owl:ObjectProperty ;
   rdfs:label "is Geopolitical Dependency Of" ;
   rdfs:comment "The relation that obtains between a geopolitical dependency d and a sovereign country c if and only if the government of d is in some respects under the jurisdiction of the institutions of c" ;
   rdfs:domain building:GeopoliticalDependency ;
   rdfs:range building:SovereignCountry ;
   rdfs:subPropertyOf ies:relationship .

building:CrownDependency a rdfs:Class ;
   rdfs:label "Crown Dependency" ;
   rdfs:comment "A geopolitical dependency that (i) is a dependency of the United Kingdom, (ii) is part of the British Isles, and (iii) is under the sovereignty of the British Crown; there are three Crown dependencies: the Isle of Man, the Bailiwick of Jersey, and the Bailiwick of Guernsey" ;
   rdfs:subClassOf building:GeopoliticalDependency .

building:BritishOverseasTerritory a rdfs:Class ;
   rdfs:label "British Overseas Territory" ;
   rdfs:comment "A geopolitical dependency that (i) is a dependency of the United Kingdom, (ii) is located outside of the British Isles, and (iii) is under the rule of the United Kingdom's government; most British overseas territories are former colonies of the British Empire, but some are not: for example, the British Antarctic Territory and South Georgia and the South Sandwich Islands, both of which are mainly used for research purposes" ;
   rdfs:subClassOf building:GeopoliticalDependency .

building:isInstitutionalSuccessorOf a owl:ObjectProperty ;
   rdfs:label "is Institutional Successor Of" ;
   rdfs:comment "The sub-relation of ies:successorTo that obtains between two regions of a country (ies:RegionOfCountry) if and only if the former has replaced the latter within the administrative hierarchy of a country" ;
   rdfs:domain ies:RegionOfCountry ;
   rdfs:range ies:RegionOfCountry ;
   rdfs:subPropertyOf ies:successorTo .

building:Waterbody a rdfs:Class ;
   rdfs:label "Waterbody" ;
   rdfs:comment "A geographic feature that is a large water mass; for example, a sea or a stream - conceived as a mass of water, rather than the channel conveying it" ;
   rdfs:subClassOf ies:GeographicFeature .

building:TidalWaterbody a rdfs:Class ;
   rdfs:label "Tidal Water Body" ;
   rdfs:comment "A waterbody that is affected by tides - that is, cyclical changes in the water level due to the gravitational pull of the Moon and the Sun; besides seas, oceans, and their parts, other types of waterbody can also be tidal, including rivers and lakes that are connected to the sea" ;
   rdfs:subClassOf building:Waterbody .

building:NonTidalWaterbody a rdfs:Class ;
   rdfs:label "Non-Tidal Water Body" ;
   rdfs:comment "A waterbody that is not affected by tides" ;
   rdfs:subClassOf building:Waterbody .
