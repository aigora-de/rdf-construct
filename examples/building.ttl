    rdfs:subClassOf building:Structure .

building:Crypt a rdfs:Class ;
  rdfs:label "Crypt" ;
  rdfs:comment "A Sepulchre that is an underground or partly underground chamber used to inter the dead." ;
  rdfs:subClassOf building:Sepulchre .

building:FogHorn a rdfs:Class ;
  rdfs:label "Fog Horn" ;
