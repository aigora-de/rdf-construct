############################  Building parts code set analysis ################################
building:Tower a rdfs:Class ;
    rdfs:label "Tower" ;
    rdfs:comment "A building that is tall and vertically oriented, typically rising significantly above its surroundings, often serving purposes such as observation, defence, habitation, or communication." ;
    rdfs:subClassOf building:Structure .


building:AirTrafficControlTower a rdfs:Class ;
  rdfs:label "Air Traffic Control Tower" ;
  rdfs:comment "A tower from which aircraft movements are monitored and directed." ;
  rdfs:subClassOf building:Tower .

building:BoatHouse a rdfs:Class ;
  rdfs:label "Boat House" ;
  rdfs:comment "A Building that is positioned by water and designed for the shelter and storage of boats, often with direct water access." ;
  rdfs:subClassOf building:Building .

building:Building a rdfs:Class ;
  rdfs:label "Building" ;
  rdfs:comment "A Building that includes a roof and walls forming enclosed space suitable for occupation or use." ;
  rdfs:subClassOf building:Building .

building:Castle a rdfs:Class ;
  rdfs:label "Castle" ;
