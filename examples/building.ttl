building:SingleUnitCentralHeatingSystem a rdfs:Class ;
    rdfs:label "Single Unit Central Heating System"@en-gb ;
    rdfs:comment "A central heating system that is designed for heating one structure unit" ;
    rdfs:subClassOf building:CentralHeatingSystem .

building:PortableHeater a rdfs:Class ;
    rdfs:label "Portable Heater" ;
    rdfs:comment "A heating system that is designed to be easily moved and is typically used for heating a space of a structure unit" ;
    rdfs:subClassOf building:HeatingSystem .

building:ElectricCeilingHeater a rdfs:Class ;
    rdfs:label "Electric Ceiling Heater" ;
    rdfs:comment:"A `building:HeatingSystem` that is mounted on the ceiling and uses electricity to generate heat, typically through resistance coils or infrared radiation." ;
