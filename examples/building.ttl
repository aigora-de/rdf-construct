building:locationBoundaryPoint a owl:ObjectProperty ;
    rdfs:label "location boundary point"@en-gb ;
    rdfs:comment "A relationship between an `ies:Location` and an `ies:GeoPoint` that holds when the point is on the boundary of the location." ;
    skos:note "we need a more thorough description of regions and boundaries" ;
    rdfs:domain ies:Location ;
    rdfs:range ies:GeoPoint .

#############   TrustMark   #############
building:TrustMark a ies:Organisation ;
    rdfs:label "TrustMark"@en-gb ;
    rdfs:comment "TrustMark (2005) Limited is a not-for-profit organisation, operating under a Master Licence Agreement, issued by the Government's Department for Business & Trade." .

building:TrustMarkAssessment a rdfs:Class ;
    rdfs:label "TrustMark Assessment"@en-gb ;
    rdfs:comment "A `building:EnergyPerformanceAssessment` that assesses the state of a building in the UK using the TrustMark quality scheme for home improvements." ;
    rdfs:subClassOf building:EnergyPerformanceAssessment .

building:Resident a rdfs:Class ;
    rdfs:label "Resident"@en-gb ;
    rdfs:comment "A temporal state of a Responsible Actor whose legal domicile is a `building:StructureUnitState`." ;
    rdfs:subClassOf ies:InResidence .

building:hasDomicileIn a owl:ObjectProperty ;
    rdfs:label "is Located In"@en-gb ;
    rdfs:domain building:Resident ;
    rdfs:range building:StructureUnitState ;
    rdfs:comment "A sibling property of `ies:visits` connecting a `building:Resident` to a location (`building:StructureUnitState`) in which it has legal domicile." ;
    rdfs:subPropertyOf ies:inLocation .

building:residesIn a owl:ObjectProperty ;
    rdfs:label "Resides In"@en-gb ;
    rdfs:domain ies:PersonState ;
    rdfs:range building:ResidentialDwelling ;
    rdfs:comment "A relation connecting a `ies:PersonState` to a location (`building:ResidentialDwelling`) in which they live." ;
    rdfs:subPropertyOf building:hasDomicileIn .

building:Tenure a rdfs:Class ;
    rdfs:label "Tenure"@en-gb ;
    rdfs:comment "A classifier of the temporal state of `building:StructureUnit` during which a property is occupied for a particular purpose, such as residence or business. It can refer to the ownership or rental status of a property. Tenure is also the legal rights associated with that occupation." ;
    skos:example "Owner Occupied", "Private Rental Sector", "Social Housing", "Other" ;
    rdfs:subClassOf building:ClassOfStructureUnitState .

building:OwnerOccupied a building:Tenure ;
    rdfs:label "Owner Occupied"@en-gb ;
    rdfs:comment "A `building:Tenure` where the occupant owns the property." ;
    rdfs:subClassOf building:StructureUnitState .

building:PrivateRentalSector a building:Tenure ;
    rdfs:label "Private Rental Sector"@en-gb ;
    rdfs:comment "A `building:Tenure` where the occupant rents the property from a private landlord." ;
    rdfs:subClassOf building:StructureUnitState .

building:SocialHousing a building:Tenure ;
    rdfs:label "Social Housing"@en-gb ;
    rdfs:comment "A `building:Tenure` where the occupant rents the property from a social landlord, such as a local authority or housing association." ;
    rdfs:subClassOf building:StructureUnitState .

building:OtherTenure a building:Tenure ;
    rdfs:label "Other Tenure"@en-gb ;
    rdfs:comment "A `building:Tenure` that does not fit into the other categories, such as temporary accommodation or shared ownership." ;
    rdfs:subClassOf building:StructureUnitState .

building:TMLicensee a rdfs:Class ;
    rdfs:label "Trust Mark Licensee" ;
    rdf:comment "An `ies:OrganisationState` that asserts that the organisation has been certified under the Trustmark Quality Scheme" ;
    rdfs:subClassOf ies:OrganisationState .

building:Installer a rdfs:Class ;
    rdfs:label "Installer"@en-gb ;
    rdfs:comment "An `ies:EventParticipant` where a `ies:ResponsibleActor` has a role in a `building:ConstructionProject`." ;
    rdfs:subClassOf ies:EventParticipant .

building:TMInstaller a rdfs:Class ;
    rdfs:label "TrustMark Installer"@en-gb ;
    rdfs:comment "An `building:Installer` that has been certified under the TrustMark quality scheme." ;
    rdfs:subClassOf building:Installer .

building:TMLicence a rdfs:Class ;
    rdfs:label "TrustMark Licence"@en-gb ;
    rdfs:comment "A `ies:IndividualDocument` certifying the competency and quality workmanship of a TrustMark Registered Business." ;
    rdfs:subClassOf ies:IndividualDocument .

building:TMLicenceNumber a rdfs:Class ;
    rdfs:label "TrustMark Licence Number"@en-gb ;
    rdfs:comment "The licence number a business obtains upon completing the TrustMark certification." ;
    rdfs:subClassOf ies:Identifier .

building:TMIdentifier a rdfs:Class ;
    rdfs:label "TrustMark Identifier"@en-gb ;
    rdfs:comment "A `ies:Identifier` that is used to identify a `building:TrustMarkAssessment`." ;
    rdfs:subClassOf ies:Identifier .

building:hasTMLicence a owl:ObjectProperty ;
    rdfs:label "has TrustMark Licence"@en-gb ;
    rdfs:domain building:TMLicensee ;
    rdfs:range building:TMLicence ;
    rdfs:comment "A relation between a `building:TMInstaller` and an asset (TrustMark licence) in their possession." ;
    rdfs:subPropertyOf ies:inPossessionOf .

building:EnergySupplier a rdfs:Class ;
    rdfs:label "Energy Supplier"@en-gb ;
    rdfs:comment "A business supplying energy to a `building:Structure`. " ;
    rdfs:subClassOf ies:Supplier .

building:ECOSupplier a rdfs:Class ;
    rdfs:label "ECO Supplier"@en-gb ;
    rdfs:comment "An `building:EnergySupplier` that has an obligation under the Energy Company Obligation (ECO) scheme." ;
    rdfs:seeAlso building:ECOSupplierReference ;
    rdfs:subClassOf building:EnergySupplier .

building:ECOSupplierReference a rdfs:Class ;
    rdfs:label "ECO Supplier Reference"@en-gb ;
    rdfs:comment "The `ies:Identifier` of a business supplying energy to a `building:Structure` under the Energy Company Obligation (ECO) scheme." ;
    skos:example "Avro Energy", "British Gas", "Bulb", "E", "EDF Energy", "E.ON Energy", "Green Network", "Green Star", "npower", "Octopus Energy", "OVO", "SSE", "ScottishPower", "Shell Energy (formerly First Utility)", "The Co-operative Energy", "The Utility Warehouse", "Utilita" ;
    rdfs:subClassOf ies:Identifier .

building:StandardAssessmentProcedure a rdfs:Class ;
    rdfs:label "Standard Assessment Procedure"@en-gb ;
    rdfs:comment "The Standard Assessment Procedure (SAP) is the UK Government's recommended method for measuring the energy rating of residential properties." ;
    rdfs:subClassOf building:EnergyPerformanceAssessment     .

building:RdSAP a rdfs:Class ;
    rdfs:label "Reduced Data Standard Assessment Procedure"@en-gb ;
    rdfs:comment "The Reduced Data Standard Assessment Procedure (RdSAP) is a simplified version of the SAP used for residential properties." ;
    skos:note "Often abbreviated rdSAP" ;
    rdfs:subClassOf building:EnergyPerformanceAssessment .

building:RdSAPTransaction a rdfs:Class ;
    rdfs:label "RdSAP Transaction"@en-gb ;
    rdfs:comment "A `ies:BusinessEvent` specifying the purpose for which the RdSAP energy performance assessment is conducted." ;
    rdfs:subClassOf ies:BusinessEvent .

building:ClassOfRenovationProject a rdfs:Class ;
    rdfs:label "Class Of Renovation Project"@en-gb ;
    rdfs:comment "An `ies:ClassOfEvent` that is a classifier of `building:RenovationProject`." ;
    rdfs:subClassOf ies:ClassOfEvent .

building:RdSAPWorkType a rdfs:Class ;
    rdfs:label "RdSAP Work Type"@en-gb ;
    rdfs:comment "The type of renovation project classified according to the RdSAP. Instances of this class are `rdfs:subClassOf` `building:RenovationProject`." ;
    rdfs:subClassOf building:ClassOfRenovationProject .

building:GasSupplySystem a rdfs:Class ;
    rdfs:label "Gas Supply System"@en-gb ;
    skos:altLabel "Gas Services"@en-gb ;
    rdfs:comment "A technical system for the supply of gas to a building." ;
    rdfs:subClassOf building:TechnicalSystem .

building:ElectricitySupplySystem a rdfs:Class ;
    rdfs:label "Building Services Electricity"@en-gb ;
    skos:altLabel "Electricity Services"@en-gb ;
    rdfs:comment "A technical system for the supply of electricity to a building." ;
    rdfs:subClassOf building:TechnicalSystem .

building:HotWaterSupplySystem a rdfs:Class ;
    rdfs:label "Building Services Hot Water"@en-gb ;
    skos:altLabel "Hot Water Services"@en-gb ;
    rdfs:comment "A technical system for the supply of hot water to a building." ;
    rdfs:subClassOf building:TechnicalSystem .

# propertyType in the data dictionary, as specified in RdSAP Appendix S1.
building:ParkHome a building:StructureUnitType ;
    rdfs:label "Park Home"@en-gb ;
    rdfs:comment "An instance `building:StructureType` that is a single-storey, detached residential `building:StructureUnit` situated within private or local authority-run parks." ;
    rdfs:subClassOf building:StructureUnitState .

building:hasPropertyType a owl:ObjectProperty ;
    rdfs:label "has Property Type"@en-gb ;
    rdfs:comment "A relation between a `building:StructureUnitState` and a `building:ClassOfStructureUnitState` that specifies the type of the structure unit as in RdSAP Appendix S1, that is using one instance of `building:StructureUnitType` and one of `building:BuiltForm`." ;
    rdfs:domain building:StructureUnit ;
    rdfs:range building:ClassOfStructureUnitState ;
    rdfs:subPropertyOf ies:relationship .

# PropertyConstruction in the data dictionary refers to PAS 2023-19 Table B.1, criterion 5
# For the following 5 entities, I have used the same design pattern of Class Of Structure Unit State.
building:StructureUnitByRiskGrade a rdfs:Class ;
    rdfs:label "Structure Unit by Risk Grade"@en-gb ;
    rdfs:comment "A `building:ClassOfStructureUnit` that is a classifier of `building:StructureUnit` by the risk grade." ;
    rdfs:subClassOf building:ClassOfStructureUnitState .

building:hasBuiltPart a owl:ObjectProperty ;
    rdfs:label "has Built Part"@en-gb ;
    rdfs:comment "A relationship connecting the state of a built entity to a classifier of a built entity." ;
    rdfs:domain ies:ClassOfState ;
    rdfs:range building:ClassOfBuiltEntity ;
    skos:example "A `building:StructureUnitState` comprises (`building:hasBuiltPart`) a built entity such as `building:CavityWall`." ;
    rdfs:subPropertyOf ies:relationship .

building:Conventional a building:StructureUnitByRiskGrade ;
    rdfs:label "Conventional"@en-gb ;
    rdfs:comment "An instance of `building:StructureUnitByRiskGrade` specified in PAS 2035:19 Table B.1, which means masonry cavity wall construction (brick and/or block) with or without render, tile hanging or other external cladding." ;
    building:hasBuiltPart building:CavityWall ;
    skos:example "Conventional, not high-rise, not protected" ;
    rdfs:subClassOf building:StructureUnitState .

building:Traditional a building:StructureUnitByRiskGrade ;
    rdfs:label "Traditional"@en-gb ;
    rdfs:comment "An instance of `building:StructureUnitByRiskGrade` specified in PAS 2035:19 Table B.1, which means constructed with solid brick or stone walls, or timber-framed walls with any infill." ;
    building:hasBuiltPart building:SolidBrick, building:GraniteOrWhinstone, building:TimberFrame ;
    skos:example "Traditional, not protected" ;
    rdfs:subClassOf building:StructureUnitState .

building:System-Built a building:StructureUnitByRiskGrade ;
    rdfs:label "System-Built"@en-gb ;
    rdfs:comment "An instance of `building:StructureUnitByRiskGrade` specified in PAS 2035:19 Table B.1, which means frame (timber, steel or concrete) and pre-fabricated panel (concrete or timber, or a combination) construct ion, or timber-framed construction with brick or stone external cladding." ;
    building:hasBuiltPart building:SystemBuilt ;
    skos:example "System-built, not high-rise, not protected" ;
    rdfs:subClassOf building:StructureUnitState .

building:HighRise a building:StructureUnitByRiskGrade ;
    rdfs:label "High rise"@en-gb ;
    rdfs:comment "An instance of `building:StructureUnitByRiskGrade` specified in PAS 2035:19 Table B.1, which means over 12 metres or over four storeys in height above the ground." ;
    skos:example "High-rise, any construction" ;
    rdfs:subClassOf building:StructureUnitState .

building:Protected a building:StructureUnitByRiskGrade ;
    rdfs:label "Protected"@en-gb ;
    rdfs:comment "An instance of `building:StructureUnitByRiskGrade` specified in PAS 2035:19 Table B.1, which includes Listed Buildings and buildings in Conservation Areas or World Heritage Sites" ;
    skos:example "Protected, any construction or built form" ;
    rdfs:subClassOf building:StructureUnitState .

building:ConstructionProject a rdfs:Class ;
    rdfs:label "Construction Project"@en-gb ;
    rdfs:comment "An `ies:Event`that is the creation, maintenance, or destruction of a `building:Structure` or `building:StructureUnit`." ;
    rdfs:subClassOf ies:Event .

building:NewBuild a rdfs:Class ;
    rdfs:label "New Build"@en-gb ;
    rdfs:comment "A `building:ConstructionProject` that covers the construction of a new `building:structure`s." ;
    rdfs:subClassOf building:ConstructionProject .

