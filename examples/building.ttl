  rdfs:subClassOf building:Tower .

building:BoreHole a rdfs:Class ;
  rdfs:label "Bore Hole" ;
  rdfs:comment "A Structure that encloses a drilled hole used to extract substances such as water, oil, or gas." ;
  rdfs:subClassOf building:Structure .

building:Bridge a rdfs:Class ;
    rdfs:label "Bridge" ;
    rdfs:comment "A structure that is built to span a physical obstacle, such as a river, valley, or road, providing a passage for pedestrians, vehicles, or utilities." ;
    rdfs:subClassOf building:Structure .

building:CableBridge a rdfs:Class ;
  rdfs:label "Cable Bridge" ;
  rdfs:comment "A bridge built to carry cables over an obstacle such as a river or road." ;
  rdfs:subClassOf building:Bridge .

building:CastleWall a rdfs:Class ;
  rdfs:label "Castle Wall" ;
  rdfs:comment "A defensive wall forming part of a castle or fortified complex." ;
  rdfs:subClassOf building:Wall .

building:ClockTower a rdfs:Class ;
  rdfs:label "Clock Tower" ;
  rdfs:comment "A tall tower incorporating one or more clock faces positioned near its summit." ;
  rdfs:subClassOf building:Tower .

building:Sepulchre a rdfs:Class ;
    rdfs:label "Sepulchre" ;
