  rdfs:subClassOf building:Building .

building:PoliceCallBox a rdfs:Class ;
  rdfs:label "Police Call Box" ;
  rdfs:comment "A Building that is a small kiosk designed to house a telephone for contacting the police." ;
  rdfs:subClassOf building:Building .

building:RoofedBandstand a rdfs:Class ;
  rdfs:label "Roofed Bandstand" ;
  rdfs:comment "A Building that is a roofed performance platform, typically open at the sides, for public musical events." ;
  rdfs:subClassOf building:Building .

building:Shelter a rdfs:Class ;
  rdfs:label "Shelter" ;
  rdfs:comment "A structure that provides cover for people against weather or adverse conditions." ;
  rdfs:subClassOf building:Structure .

building:SignalBox a rdfs:Class ;
  rdfs:label "Signal Box" ;
  rdfs:comment "A Building that is a distinctive railway‑side structure historically housing the mechanisms for points and signals." ;
  rdfs:subClassOf building:Building .

building:StaticCaravanOrMobileHome a rdfs:Class ;
  rdfs:label "Static Caravan Or Mobile Home" ;
  rdfs:comment "A Building that is a relocatable dwelling originally designed for movement but positioned for residential or holiday use." ;
  rdfs:subClassOf building:Building .

building:Tomb a rdfs:Class ;
  rdfs:label "Tomb" ;
  rdfs:comment "A Sepulchre a roofed edifice used for the interment of human remains." ;
  rdfs:subClassOf building:Sepulchre .

building:WheelHouse a rdfs:Class ;
  rdfs:label "Wheel House" ;
  rdfs:comment "A Building that houses the lifting gear associated with mining operations." ;
  rdfs:subClassOf building:Building .

building:Windmill a rdfs:Class ;
  rdfs:label "Windmill" ;
  rdfs:comment "A Building that is designed to be wind‑driven for milling or pumping and typically comprises an occupiable tower with sails." ;
