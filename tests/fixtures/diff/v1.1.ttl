@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.org/> .

# Ontology metadata
ex:buildingOntology a owl:Ontology ;
    rdfs:label "Building Ontology"@en ;
    rdfs:comment "An example ontology for buildings and structures."@en .  # Modified comment

# Classes
ex:Building a owl:Class ;
    rdfs:label "Building"@en ;
    rdfs:comment "A constructed physical structure."@en ;  # Added comment
    rdfs:subClassOf owl:Thing .

ex:Room a owl:Class ;
    rdfs:label "Room"@en ;
    rdfs:subClassOf ex:Building .

# DeprecatedStructure removed (was deprecated)

# New class added
ex:SmartBuilding a owl:Class ;
    rdfs:label "Smart Building"@en ;
    rdfs:comment "A building with integrated smart technology."@en ;
    rdfs:subClassOf ex:Building .

# Object Properties
ex:hasRoom a owl:ObjectProperty ;
    rdfs:label "has room"@en ;
    rdfs:domain ex:Building ;
    rdfs:range ex:Room .

ex:partOf a owl:ObjectProperty ;
    rdfs:label "part of"@en ;
    rdfs:domain ex:Room ;
    rdfs:range ex:Building .

# Datatype Properties
ex:hasName a owl:DatatypeProperty ;
    rdfs:label "has name"@en ;
    rdfs:domain ex:Building ;
    rdfs:range xsd:string .

ex:constructionYear a owl:DatatypeProperty ;
    rdfs:label "construction year"@en ;
    rdfs:domain ex:Building ;
    rdfs:range xsd:integer .

# New property added
ex:energyRating a owl:DatatypeProperty ;
    rdfs:label "energy rating"@en ;
    rdfs:comment "Energy efficiency rating (A-G scale)."@en ;
    rdfs:domain ex:Building ;
    rdfs:range xsd:string .
